#include <iostream>
#include <vector>
#include <queue>
#include <stack>
#include <string>
#include <climits>
#include <stdio.h>
#include <string.h>
#include <cassert>
#include <algorithm>
#include <stack>
#include <map>
#include <list>
#include <set>
#include <math.h>

using namespace std;

struct node
{
    int data;
    node *left, *right;
}*root=NULL;

void insert(int d)
{
    node *p=new node();
    p->data=d;
    p->left=NULL;
    p->right=NULL;
    if(root==NULL)
        root=p;
    else
    {
        node *temp=root;
        node *parent=new node();
        while (temp!=NULL)
        {
            parent=temp;
            if(d<parent->data)
                temp=temp->left;
            else
                temp=temp->right;
        }
        if(d<parent->data)
            parent->left=p;
        else
            parent->right=p;
    }
}

void inorder(node *p)
{
    if(p==NULL)
        return;
    inorder(p->left);
    cout<<p->data<<" ";
    inorder(p->right);
}

void inord(node *p, int *a)
{
    if(p==NULL)
        return;
    static int i=0;
    inord(p->left, a);
    a[i++]=p->data;
    inord(p->right, a);
}

bool isLeaf(node *p, int d)
{
    if(p==NULL)
        return false;
    if(p->data==d && p->left==NULL && p->right==NULL)
        return true;
    if(d<p->data)
        return isLeaf(p->left, d);
    else
        return isLeaf(p->right, d);
}

int height(node *p)
{
    if(p==NULL)
        return 0;
    return  1+ max(height(p->left), height(p->right));
}

bool isDead(node *p)
{
    if(p==NULL)
        return true;
    int i,h=height(p);
    int *a=new int[h];
    inord(p, a);
    if(a[0]==1 && a[1]==2)
        return true;
    for (i=1; i<h-1; i++)
    {
        if (a[i]==a[i-1] && a[i]==a[i+1])
        {
            if(isLeaf(p, a[i]))
                return true;
        }
    }
    return false;
}

int main(int argc, const char * argv[])
{
    int ele,n,i;
    cin>>n;
    for (i=0; i<n ; i++)
    {
        cin>>ele;
        insert(ele);
    }
    cout<<"\nInorder: ";
    inorder(root);
    if(isDead(root))
        cout<<"\nYes";
    else
        cout<<"\nNo";
    return 0;
}
